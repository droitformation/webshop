{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
		"php": ">=7.0.0",
		"laravel/framework": "5.5.*",
		"fideloper/proxy": "~3.3",
        "laravelcollective/html": "5.5.*",
        "guzzlehttp/guzzle": "6.0.*",
		"barryvdh/laravel-debugbar": "^3.1",
		"intervention/image": "^2.4",
        "gloudemans/shoppingcart": "2.4.0",
        "barryvdh/laravel-dompdf": "0.8.x@dev",
        "maatwebsite/excel": "2.1.*",
        "venturecraft/revisionable": "1.*",
        "simplesoftwareio/simple-qrcode": "1.5.*",
		"designPond/config-registry": "0.1.x-dev",
        "baum/baum": "~1.1",
        "inlinestyle/inlinestyle": "1.*",
		"mailjet/mailjet-apiv3-php": "^1.2",
		"vinkla/alert": "^2.2",
		"spatie/laravel-backup": "^5.1",
		"shvetsgroup/laravel-email-database-log": "^1.0",
		"msurguy/honeypot": "dev-master",
		"mailgun/mailgun-php": "^2.3.4",
		"php-http/guzzle6-adapter": "^1.1",
		"rguedes/pdfmerger": "^1.0"
	},
	"require-dev": {
		"doctrine/dbal": "2.5.0",
		"filp/whoops": "~2.0",
		"fzaninotto/faker": "1.6.0",
		"laravel/dusk": "^2.0",
		"mockery/mockery": "0.9.*",
		"phpunit/phpunit": "~6.0"
	},
	"autoload": {
		"classmap": [
			"database/seeds",
			"database/factories"
		],
		"psr-4": {
			"App\\": "app/",
			"Box\\Spout\\": "vendor/box/spout/src/Spout/"
		},
		"files": [
			"tests/factories/ObjectFactory.php",
			"app/Support/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"scripts": {
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover"
		]
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
	}
}
